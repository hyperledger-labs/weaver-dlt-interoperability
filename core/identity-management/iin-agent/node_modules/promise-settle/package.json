  ],
  "author": "Patrick Gaubatz <patrick@gaubatz.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pgaubatz/node-promise-settle/issues"
  },
  "homepage": "https://github.com/pgaubatz/node-promise-settle#readme",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^11.2.0",
    "mocha": "^2.2.4",
    "native-promise-only": "^0.8.1"
  }
}
